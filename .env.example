# AWS Configuration
AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID and AWS_SECRET_ACCESS_KEY should be set via AWS CLI or IAM roles
# DO NOT commit actual credentials to version control

# Cognito Configuration
COGNITO_USER_POOL_ID=us-east-1_example
COGNITO_CLIENT_ID=your-client-id
COGNITO_LEGACY_POOL_ID=us-east-1_legacy
COGNITO_NEW_POOL_ID=us-east-1_new

# Frontend Configuration (for Vite)
VITE_USER_POOL_ID=us-east-1_example
VITE_USER_POOL_CLIENT_ID=your-client-id
VITE_AWS_REGION=us-east-1
VITE_MFA_DEADLINE=2025-09-01

# MFA Migration Settings
MFA_MIGRATION_DEADLINE=2025-09-01
MFA_WARNING_DAYS=30,14,7,3,1
MFA_GRACE_PERIOD_DAYS=7
MFA_ENABLED_METHODS=SMS,TOTP,EMAIL

# Notification Settings
EMAIL_FROM=noreply@example.com
SMS_REGION=us-east-1

# Application Settings
NODE_ENV=development
PORT=3000
LOG_LEVEL=info